{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base",
        ":timezone(CET)",
        "schedule:nonOfficeHours",
        ":disableRateLimiting",
        ":disableDependencyDashboard"
    ],
    "packageRules": [
        {
            "matchDepTypes": ["action"],
            "pinDigests": true,
            "rangeStrategy": "pin",
            "rollbackPrs": true
        },
        {
            "matchDatasources": ["maven"],
            "registryUrls": ["https://repox.jfrog.io/artifactory/sonarsource-releases", "https://repox.jfrog.io/artifactory/central","https://repox.jfrog.io/artifactory/plugins.gradle.org"]
        }
    ],
    "hostRules": [
        {
            "matchHost": "repox.jfrog.io",
            "encrypted": {
                "token": ""
            }
        }
    ]
}
